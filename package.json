  "version": "0.6.0",
  "description": "MCP Server for Vertex AI Imagen image generation",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "vertexai-imagen-mcp-server": "build/index.js"
  },
  "scripts": {
