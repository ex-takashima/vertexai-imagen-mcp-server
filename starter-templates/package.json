{
  "name": "image-generation-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for AI image generation with history management",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "image-gen-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsc && node build/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "image-generation",
    "ai",
    "openai",
    "stable-diffusion",
    "history-management"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "better-sqlite3": "^11.0.0",
    "sharp": "^0.33.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "optionalDependencies": {
    "openai": "^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
